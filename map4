<script


src="http://maps.googleapis.com/maps/api/js">

</script>

<script>


var myLatLng=new google.maps.LatLng(-37,175);

function initialize()
{
var mapProp = {
  center:myLatLng,
  zoom:7,
  mapTypeId:google.maps.MapTypeId.ROADMAP
  };

var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);

var marker=new google.maps.Marker({
  position:myLatLng,
  icon:'5.png'
  });

marker.setMap(map);

var infowindow = new google.maps.InfoWindow({
  content:"This is where i took my Photograph in Janurary 2015. I was lucky to catch the sunset at its nicest. Beautiful place to visit, beautiful place to take photo!"
  });



infowindow.open(map,marker);
}


google.maps.event.addDomListener(window, 'load', initialize);

var map=new google.maps.Map(document.getElementById("googleMap"), mapProp);
google.maps.event.addDomListener(window, 'load', initialize);



</script>
